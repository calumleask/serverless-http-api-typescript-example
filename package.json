{
  "name": "serverless-http-api-typescript-example",
  "version": "1.0.0",
  "description": "An API for procedurally generating content",
  "main": "webpack.config.js",
  "scripts": {
    "pretest": "npm run lint",
    "test": "jest",
    "jest": "jest",
    "coverage": "jest --coverage",
    "clean": "rm -rf .webpack",
    "start": "sls offline start --host=0.0.0.0",
    "lint": "eslint --ext .js,.ts .",
    "format": "prettier --ignore-path .gitignore --write \"**/*.+(js|jsx|ts|tsx|json)\"",
    "deploy": "sls deploy"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/calumleask/serverless-http-api-typescript-example.git"
  },
  "author": "Calum Leask",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/calumleask/serverless-http-api-typescript-example/issues"
  },
  "homepage": "https://github.com/calumleask/serverless-http-api-typescript-example#readme",
  "devDependencies": {
    "@types/aws-lambda": "^8.10.95",
    "@types/jest": "^27.4.1",
    "@typescript-eslint/eslint-plugin": "^5.21.0",
    "@typescript-eslint/parser": "^5.21.0",
    "aws-sdk": "^2.1125.0",
    "clean-webpack-plugin": "^4.0.0",
    "eslint": "^8.14.0",
    "eslint-config-prettier": "^8.5.0",
    "jest": "^27.5.1",
    "merge": "^2.1.1",
    "prettier": "^2.6.2",
    "serverless": "^3.16.0",
    "serverless-offline": "^8.7.0",
    "serverless-webpack": "^5.7.0",
    "ts-jest": "^27.1.4",
    "ts-loader": "^9.3.0",
    "typescript": "^4.6.4",
    "webpack": "^5.72.0",
    "webpack-node-externals": "^3.0.0"
  },
  "dependencies": {
    "lambda-hooks": "^0.2.1",
    "yup": "^0.32.11"
  }
